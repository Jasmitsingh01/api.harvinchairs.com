{
    "name": "redq/marvel",
    "type": "project",
    "description": "Marvel, a complete ecommerce engine for both rest and graphql",
    "keywords": [
        "marvel",
        "graphql",
        "rest-api"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "efectn/laravel-menu-builder": "^1.5",
        "algolia/algoliasearch-client-php": "^3.3",
        "barryvdh/laravel-dompdf": "2.0.0",
        "bensampo/laravel-enum": "5.3.1",
        "cknow/laravel-money": "7.0.0",
        "cviebrock/eloquent-sluggable": "9.0.0",
        "doctrine/dbal": "3.3.7",
        "fideloper/proxy": "4.4.2",
        "fruitcake/laravel-cors": "3.0.0",
        "laravel/legacy-factories": "1.3.0",
        "laravel/scout": "^10.1",
        "laravel/socialite": "5.5.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/cache": "*",
        "imagekit/imagekit": "^3.0",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "prettus/l5-repository": "2.8.0",
        "spatie/laravel-medialibrary": "^10.0.7",
        "yajra/laravel-datatables-oracle": "^10.6",
        "league/flysystem-aws-s3-v3": "3.1.1",
        "maatwebsite/excel": "3.1.44",
        "messagebird/php-rest-api": "3.1.2",
        "mll-lab/graphql-php-scalars": "5.4.0",
        "mll-lab/laravel-graphql-playground": "2.6.0",
        "mollie/laravel-mollie": "2.19.1",
        "niklasravnsborg/laravel-pdf": "4.1.0",
        "nuwave/lighthouse": "5.57.0",
        "php-http/guzzle7-adapter": "1.0.0",
        "psr/log": "2.0.0",
        "razorpay/razorpay": "2.8.4",
        "spatie/laravel-newsletter": "4.11.0",
        "spatie/laravel-permission": "5.5.5",
        "spatie/period": "2.3.3",
        "srmklive/paypal": "^3.0",
        "stripe/stripe-php": "8.11.0",
        "symfony/http-client": "6.0.9",
        "symfony/mailgun-mailer": "6.0.7",
        "twilio/sdk": "6.40.0",
        "unicodeveloper/laravel-paystack": "1.0.8",
        "kodeine/laravel-meta": "2.1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "1.3.1",
        "fakerphp/faker": "1.20.0",
        "laravel/sail": "1.15.1",
        "laravel/ui": "^3.4",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "6.2.1",
        "phpunit/phpunit": "9.5.21",
        "squizlabs/php_codesniffer": "3.7.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Indapoint\\EmailTemplate\\": "packages/indapoint/email_template/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
